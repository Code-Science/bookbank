{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": "warn",
    "prettier/prettier":"error",
    "func-names": "off",
    "class-methods-use-this": "off",
    "no-param-reassign":"off"
  }
}
